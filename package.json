{
	"name": "@solidso/inspection",
	"version": "0.1.0",
	"description": "Frontend logging library for solid.js",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./types.d.ts",
	"exports": {
		".": {
			"types": {
				"default": "./types.d.ts"
			},
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"lint": "eslint src",
		"format:check": "prettier src --check",
		"format": "prettier $(git diff --cached --name-only --diff-filter=ACMR src | sed 's| |\\\\ |g') --write --ignore-unknown",
		"build": "vite build",
		"test": "vitest run",
		"prepare": "husky",
		"commit": "cz"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/solidso/solid-inspection.git"
	},
	"private": false,
	"license": "MIT",
	"author": {
		"name": "Murat GÃ¶zel",
		"email": "murat@gozel.com.tr",
		"url": "https://muratgozel.com.tr"
	},
	"keywords": [
		"frontend-logging",
		"solid-js"
	],
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.21.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.2",
		"solid-js": "^1.9.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"peerDependencies": {
		"solid-js": "^1.9.5"
	}
}
